                        <div class="logo-element">
                            Majom
                        </div>
                    </li>
                  <li class="active">


                  </li>
                    <li>
                        <a href="#"><i class="fa fa-bar-chart-o"></i> <span class="nav-label">Projekte</span><span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">

                            <li><%=link_to 'Übersicht', overview_url, method: :get%> </li>

                        </ul>
                    </li>

                    <li>
                        <a href="#"><i class="fa fa-calendar"></i> <span class="nav-label">Termine </span></a>

                      <ul class="nav nav-second-level">

                        <li><a href="events">Übersicht </a></li>
                        <li><%=link_to 'Kalender', calendar_url, method: :get%></li>


                      </ul>
                    </li>
