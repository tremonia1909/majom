<!DOCTYPE html>
<head>
  <title>Majom| Project management</title>
  <%= stylesheet_link_tag    'application', 'bootstrap.min.css', 'font-awesome/css/font-awesome.css', 'animate.css', 'style.css', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', "plugins/fullcalendar/fullcalendar.min.js",'jquery-ui.custom.min.js', "plugins/iCheck/icheck.min.js","inspinia.js","plugins/pace/pace.min.js", "jquery-2.1.1.js","bootstrap.min.js","plugins/metisMenu/jquery.metisMenu.js","plugins/slimscroll/jquery.slimscroll.min.js", 'data-turbolinks-track' => true %>

  <%= csrf_meta_tags %>
