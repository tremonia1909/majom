        <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">

            <ul class="nav navbar-top-links navbar-right">
                <li>
                    <span class="m-r-sm text-muted welcome-message">Welcome Majom | Projektmanagement.</span>
                </li>

                <li>
                  <% if user_signed_in? %>
                  <i class="fa fa-sign-out"><%= link_to "Abmelden", destroy_user_session_path, method: :delete  %></i>
                  <% else %>
                  <% end %>
                </li>
            </ul>

        </nav>
        </div>
<%= yield %>


        </div>
</div>
    
</body>
</html>
